<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE localeData SYSTEM "http://oss.software.ibm.com/cvs/icu/~checkout~/locale/LocaleElements.dtd" >
<localeData>
	<identity>
		<version number="1.2">Various notes and changes in version 1.2</version>
		<generation date="2003-04-08" />
		<language type="ko"></language>
		<territory type="KOR">KR</territory>
	</identity>
	<localeDisplayNames>
		<keys>
			<key type="collation">collation</key>
			<key type="calendar">calendar</key>
		</keys>
	</localeDisplayNames>
		<dates>
			<calendars>
				<default type="gregorian"/>
				<calendar type="gregorian">
					<dateFormats>
						<default type="medium"/>
						<dateFormatStyle type="full">
							<dateFormat >
								<pattern>yyyy'년' M'월' d'일' EEEE</pattern>
							</dateFormat>
						</dateFormatStyle >
						<dateFormatStyle type="long">
							<dateFormat >
								<pattern>yyyy'년' M'월' d'일' EE</pattern>
							</dateFormat>
						</dateFormatStyle >
						<dateFormatStyle type="medium">
							<dateFormat >
								<pattern>yyyy.MM.dd</pattern>
							</dateFormat>
						</dateFormatStyle >
						<dateFormatStyle type="short">
							<dateFormat >
								<pattern>yy.MM.dd</pattern>
							</dateFormat>
						</dateFormatStyle >
					</dateFormats>
					<timeFormats>
						<default type="medium"/>
						<timeFormat type="full">
							<pattern>a h'시' mm'분' ss'초' z</pattern>
						</timeFormat>
						<timeFormat type="long">
							<pattern>a h'시' mm'분' ss'초'</pattern>
						</timeFormat>
						<timeFormat type="medium">
							<pattern>a h:mm:ss</pattern>
						</timeFormat>
						<timeFormat type="short">
							<pattern>a h'시' mm'분'</pattern>
						</timeFormat>
					</timeFormats>
					<dateTimeFormat>
						<pattern>{1} {0}</pattern>
					</dateTimeFormat>
				</calendar>
			</calendars>
		</dates>
		<numbers>
			<numberFormats>
				<default type="decimal"/>
				<numberFormatStyle type="decimal" >
					<numberFormat >
						<pattern>#,##0.###</pattern>
						<pattern>-#,##0.###</pattern>
					</numberFormat>
				</numberFormatStyle  >
				<numberFormatStyle type="currency" >
					<numberFormat >
						<pattern>¤#,##0.00</pattern>
						<pattern>-¤#,##0.00</pattern>
					</numberFormat>
				</numberFormatStyle  >
				<numberFormatStyle type="percent" >
					<numberFormat >
						<pattern>#,##0%</pattern>
					</numberFormat>
				</numberFormatStyle  >
				<numberFormatStyle type="scientific" >
					<numberFormat >
						<pattern>#E0</pattern>
					</numberFormat>
				</numberFormatStyle  >
			</numberFormats>
			<currencies>
				<default type="KRW"/>
				<currency type="KRW">
					<symbol>₩</symbol>
					<displayName>KRW</displayName>
					<decimal>.</decimal>
				</currency>
			</currencies>
		</numbers>
</localeData>
