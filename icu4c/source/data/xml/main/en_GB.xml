					<dateTimeFormatLength >
						<dateTimeFormat>
							<pattern>{1} {0}</pattern>
						</dateTimeFormat>
					</dateTimeFormatLength >
				</dateTimeFormats>
			</calendar>
		</calendars>
		<timeZoneNames>
		</timeZoneNames>
